        "ffmpeg-static": "^4.4.1",
        "libsodium-wrappers": "^0.7.9",
        "moment": "^2.29.1",
        "pg": "^8.7.3",
        "typeorm": "^0.2.42",
        "youtube-search-without-api-key": "^1.0.7",
        "ytdl-core": "^4.10.1",
        "ytpl": "^2.2.3"
    },
    "devDependencies": {
        "@types/node": "^17.0.14",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    }
