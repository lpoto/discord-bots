{
    "name": "music-bot",
    "version": "1.0.2",
    "description": "A discord bot for playing music.",
    "author": "Luka Potočnik",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lpoto/discord-music-bot.git"
    },
    "main": "src/index.ts",
    "scripts": {
        "dev": "nodemon src/index.ts",
        "build": "tsc -p .",
        "start": "node dist/index.js",
